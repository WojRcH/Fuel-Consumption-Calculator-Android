<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.w.kalkulatorspalania">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fuelicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:testOnly="false"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Kalkulator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".spal_100_km" android:label="@string/app_name"></activity>
        <activity android:name=".koszt" android:label="@string/app_name"></activity>
    </application>

</manifest>